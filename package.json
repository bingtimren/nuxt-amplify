{
  "name": "nuxt-amplify",
  "version": "1.0.0",
  "description": "AWS Amplify integration with Nuxt made easy",
  "license": "MIT",
  "contributors": [
    {
      "name": "Bing Ren <bing.ren@gmail.com>"
    }
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/bingtimren/nuxt-amplify",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nuxt test/fixture",
    "lint": "eslint lib test",
    "test": "npm run lint && jest",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "files": [
    "lib"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "dependencies": {
    "aws-amplify": "^1.0.6"
  },
  "devDependencies": {
    "nuxt": "latest",
    "codecov": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "eslint-plugin-vue": "latest",
    "jest": "latest",
    "jsdom": "latest",
    "standard-version": "latest"
  }
}
